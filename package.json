{
  "name": "powerapp-backend",
  "version": "1.0.0",
  "description": "Backend service for power outage schedule parser (Telegram-source) and public schedule API for Zaporizhzhya region.",
  "type": "module",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "parse:manual": "node src/tools/force-parse.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write src/**/*.js"
  },
  "keywords": [
    "Energy",
    "Ukraine",
    "PowerGrid",
    "Outage",
    "Parser",
    "Express",
    "API"
  ],
  "author": "Oleksandr Fedotov",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.7.4",
    "better-sqlite3": "^9.1.2",
    "cheerio": "^1.0.0",
    "cors": "^2.8.5",
    "dayjs": "^1.11.11",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.1.5",
    "node-cron": "^3.0.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "telegraf": "^4.16.3"
  },
  "devDependencies": {
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.2.0",
    "nodemon": "^3.1.2",
    "openapi-types": "^12.1.3",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4"
  }
}
